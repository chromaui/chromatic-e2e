{
  "name": "@chromatic-com/shared-e2e",
  "private": true,
  "version": "0.1.1",
  "description": "Shared Chromatic E2E",
  "repository": {
    "type": "git",
    "url": "https://github.com/chromaui/test-archiver.git",
    "directory": "packages/shared"
  },
  "author": "Chromatic <tom@chromatic.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./archive-storybook": {
      "types": "./dist/archive-storybook/index.d.ts",
      "import": "./dist/archive-storybook/index.mjs",
      "require": "./dist/archive-storybook/index.js"
    },
    "./utils/filePaths": {
      "types": "./dist/utils/filePaths.d.ts",
      "import": "./dist/utils/filePaths.mjs",
      "require": "./dist/utils/filePaths.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "typesVersions": {
    "*": {
      ".": [
        "index.d.ts"
      ],
      "archive-storybook": [
        "dist/archive-storybook/index.d.ts"
      ],
      "utils/filePaths": [
        "dist/utils/filePaths.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "yarn clean",
    "build": "tsup",
    "build:watch": "yarn build --watch",
    "test:unit": "jest",
    "lint": "eslint src/*",
    "prettier": "prettier"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@jest/types": "^27.0.6",
    "@playwright/test": "^1.39.0",
    "@storybook/addon-essentials": "^7.5.2",
    "@storybook/eslint-config-storybook": "^3.1.2",
    "@storybook/server-webpack5": "^7.5.2",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.4.1",
    "@types/node-fetch": "^2.6.6",
    "babel-jest": "^27.0.6",
    "concurrently": "^7.0.0",
    "eslint": "^7.32.0",
    "fs-extra": "^11.1.1",
    "jest": "^27.0.6",
    "jest-environment-jsdom": "^27.0.6",
    "lint-staged": ">=10",
    "mime": "^3.0.0",
    "node-fetch": "2",
    "playwright": "^1.32.2",
    "playwright-core": "^1.32.2",
    "prettier": "^2.3.1",
    "prompts": "^2.4.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "srcset": "^4.0.0",
    "storybook": "^7.5.2",
    "ts-dedent": "^2.2.0",
    "ts-jest": "^27.0.4",
    "tsup": "^6.7.0",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.{ts,js,css,md}": "prettier --write"
  },
  "dependencies": {
    "@segment/analytics-node": "^1.1.0",
    "rrweb-snapshot": "^2.0.0-alpha.4"
  }
}
